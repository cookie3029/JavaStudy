package day15;

public class Person {

	public enum Gender {
		MALE, FEMALE
	};

	public enum City {
		Seoul, Pusan
	};

	private String nameString;
	private int age;
	private Gender gender;
	private City city;

	public Person(String name, int age, Gender gender) {
		this.name = name;
		this.age = age;
		this.gender = gender;
	}
}
