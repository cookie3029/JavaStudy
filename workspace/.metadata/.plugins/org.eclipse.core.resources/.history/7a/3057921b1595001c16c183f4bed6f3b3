package day14;

class UseThis {
	
	int outterVar = 10;
	
	class Inner {
		int innerVar = 20;
		
		void method() {
			MyFunctionalInterface2 my = () -> {
				System.out.println("outter var : " + outterVar);
				
			};
			
			my.method();
		}
	}
	
	
}

